<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.maventy"
    android:versionCode="3"
    android:versionName="0.82" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/maventy_icon"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        
        <activity
            android:name="org.maventy.android.Main"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="org.maventy.android.Settings" >
        </activity>
        <activity android:name="org.maventy.android.app.visit.MaventyAndroid" >
        </activity>
        <activity android:name="org.maventy.android.app.visit.ShowResults" >
        </activity>
        <activity android:name="org.maventy.android.app.visit.SaveVisit" >
        </activity>
        <activity android:name="org.maventy.android.app.visit.PlotResults" >
        </activity>
        <activity android:name="org.achartengine.GraphicalActivity" >
        </activity>
        <activity android:name="org.maventy.android.app.ExploreDatabase" >
        </activity>
        <activity android:name="org.maventy.android.app.ExplorePatient" >
        </activity>
        <activity android:name="org.maventy.android.app.AddNewPatient" >
        </activity>
        <activity android:name="org.maventy.android.app.UploadVisit" >
        </activity>
        <activity android:name="org.maventy.android.app.Download" >
        </activity>
        <activity android:name="org.maventy.android.Tos" >
        </activity>
        
    </application>
</manifest>